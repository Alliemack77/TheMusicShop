;\r\n    color: $creme;\r\n}\r\n\r\n.btn-cta {\r\n    @include ctaBtnOrange;\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 1em;\r\n    font-weight: $bold;\r\n    transition-duration: 200ms;\r\n\r\n    &:hover {\r\n        color: #ff7c35;\r\n        transform: scale(2rem)\r\n    }\r\n\r\n    &.uppercase {\r\n        @include uppercase;\r\n    }\r\n\r\n}\r\n\r\n\r\n\r\n\r\n//// Typography\r\n.logo {\r\n    @include accentText;\r\n    width: min-content;\r\n    margin: 0;\r\n    font-size: $logo;\r\n}\r\n\r\n// .nav-list--item {\r\n//     font-size: clamp(1rem, 1.849vw, 2.125rem  ); //18px, 19.2px, 34\r\n\r\n//     //font-size: clamp(.75rem, 1.541vw, 1.5rem  ); //18px, 19.2px, 34\r\n// }\r\n\r\n// .text-small {\r\n//     @include smallText;\r\n// }\r\n\r\n.title {\r\n    @include accentText;\r\n    font-size: clamp(2.5rem, 4.5vw, 6rem); // ideal -->  2.875rem/46px/4.5vw\r\n    margin: 1.5em 0 0 0;\r\n    line-height: .55;\r\n\r\n}\r\n\r\n.title-secondary {\r\n    @include uppercase;\r\n    margin: 0;\r\n    padding: 0;\r\n    font-size: clamp(5rem, 8.5vw, 10rem);\r\n    font-weight: $extra-bold;\r\n    text-transform: uppercase;\r\n}\r\n\r\n.subtitle-cursive {\r\n    @include accentText;\r\n}\r\n\r\n.subtitle-accent {\r\n    @include ctaBtnOrange;\r\n    font-weight: $bold;\r\n}\r\n\r\n.subtitle-thin {\r\n    @include ctaBtnOrange;\r\n    font-weight: $thin;\r\n}\r\n\r\n\r\n//// Cards\r\n.card-title {\r\n    @include accentText;\r\n}\r\n\r\n\r\n//// Header\r\nheader {\r\n    //border: solid red 2px;\r\n    padding: 2em 1em;\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 2.5em;\r\n    \r\n    & .logo {\r\n        text-align: center;\r\n    }\r\n\r\n    & .nav-list, \r\n    .social-links {\r\n        //border: solid yellow 2px;\r\n        display: flex;\r\n        justify-content: center;\r\n        gap: 2.75em;\r\n    }\r\n\r\n    .social-link {\r\n        font-size: 1.75rem;\r\n    }\r\n\r\n    & li:nth-child(1) {order: 2;}\r\n    & li:nth-child(2) {order: 1; align-self: center;}\r\n    & li:nth-child(3) {order: 3; align-self: center;}\r\n}\r\n\r\n\r\n\r\n\r\n//// Main\r\nmain {\r\n    //border: solid red 2px;\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 5rem;\r\n    padding: 2.5em 0 4em 0;\r\n    background: linear-gradient(90deg, rgba(33,38,32,1) 0%, rgba(33,38,32,1) 15%, rgba(31,36,30,1) 15%,\r\n    rgba(31,36,30,1) 20%, rgba(33,38,32,1) 20%, rgba(33,38,32,1) 22%, rgba(31,36,30,1) 22%,\r\n    rgba(31,36,30,1) 27%, rgba(33,38,32,1) 27%, rgba(33,38,32,1) 29%, rgba(31,36,30,1) 29%,\r\n    rgba(31,36,30,1) 34%, rgba(33,38,32,1) 34%, rgba(33,38,32,1) 36%, rgba(31,36,30,1) 36%,\r\n    rgba(31,36,30,1) 41%, rgba(33,38,32,1) 41%, rgba(33,38,32,1) 43%, rgba(31,36,30,1) 43%,\r\n    rgba(31,36,30,1) 48%, rgba(33,38,32,1) 48%, rgba(33,38,32,1) 50%, rgba(31,36,30,1) 50%,\r\n    rgba(31,36,30,1) 55%, rgba(33,38,32,1) 55%, rgba(33,38,32,1) 57%, rgba(31,36,30,1) 57%,\r\n    rgba(31,36,30,1) 62%, rgba(33,38,32,1) 62%, rgba(33,38,32,1) 64%, rgba(31,36,30,1) 64%,\r\n    rgba(31,36,30,1) 69%, rgba(33,38,32,1) 69%, rgba(33,38,32,1) 71%, rgba(31,36,30,1) 71%,\r\n    rgba(31,36,30,1) 76%, rgba(33,38,32,1) 76%, rgba(33,38,32,1) 78%, rgba(31,36,30,1) 78%,\r\n    rgba(31,36,30,1) 83%, rgba(33,38,32,1) 83%);\r\n\r\n    // & section {\r\n    //     border: solid 2px purple;\r\n\r\n    // }\r\n\r\n    & .hero {\r\n\r\n        & .hero-img {\r\n            box-shadow: $img-shadow;\r\n        }\r\n\r\n        & .subtitle-group {\r\n            margin-top: 1em;;\r\n            max-width: 700px;\r\n            display: flex;\r\n            justify-content: space-between;\r\n\r\n            & :nth-child(2) {\r\n                display: flex;\r\n                align-items: flex-end;\r\n            }\r\n        }\r\n\r\n        & .medium {\r\n            font-size: 1.75rem;\r\n        }\r\n        \r\n        \r\n\r\n\r\n    }\r\n}\r\n\r\n\r\n\r\n//// Footer\r\nfooter {\r\n    //border: solid red 2px;\r\n    display: flex;\r\n    width: 100%;\r\n    min-height: 500px;\r\n    margin-top: 50px;\r\n    padding-top: 80px;\r\n    padding-bottom: 80px;\r\n    background-image: linear-gradient(86deg, #212620 25%, transparent), url(https://i.postimg.cc/N08VVdSB/IMG-7744-Edit.jpg);\r\n    background-position: 0px 0px, 50% 50%;\r\n    background-size: cover;\r\n    background-repeat: no-repeat;\r\n\r\n\r\n    & .footer__body {\r\n      //border: solid yellow 2px;\r\n      margin-left: 1em;\r\n\r\n      @media (min-width: 750px) {\r\n        margin-left: 5em;\r\n      }\r\n\r\n\r\n      & .text-small {\r\n        font-size: .75rem;\r\n      }\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n    }\r\n}\r\n","scss\\mixins.scss":"@import '../scss/variables.scss';\r\n\r\n\r\n@mixin accentText {\r\n    font-family: $font-accent;\r\n    font-weight: $thin;\r\n}\r\n\r\n@mixin smallText {\r\n    font-size: 0.875rem;\r\n    font-weight: $thin;\r\n}\r\n\r\n// uppercase text\r\n@mixin uppercase {\r\n    text-transform: uppercase;\r\n}\r\n\r\n// cta buttons\r\n@mixin ctaBtnOrange {\r\n    color: $orange;\r\n}\r\n\r\n\r\n// card body layout\r\n//card body style\r\n"},"lineCount":null}},"error":null,"hash":"3d5d75e2065cd425520c4645393b149a","cacheData":{"env":{}}}